{
    "multipart": [
		{	"when": { "OR": [
				{"facing": "north", "open": "true"}
            ]},
			"apply": { "model": "block/spruce_trapdoor_open"}
		},
		{	"when": { "OR": [
				{"facing": "east", "open": "true"}
            ]},
			"apply": { "model": "block/spruce_trapdoor_open", "y":90}
		},
		{	"when": { "OR": [
				{"facing": "south", "open": "true"}
            ]},
			"apply": { "model": "block/spruce_trapdoor_open", "y":180}
		},
		{	"when": { "OR": [
				{"facing": "west", "open": "true"}
            ]},
			"apply": { "model": "block/spruce_trapdoor_open", "y":270}
		},
		{	"when": { "OR": [
				{"facing": "north", "open": "false", "half": "bottom"}
            ]},
			"apply": { "model": "block/spruce_trapdoor"}
		},
		{	"when": { "OR": [
				{"facing": "east", "open": "false", "half": "bottom"}
            ]},
			"apply": { "model": "block/spruce_trapdoor", "y":90}
		},
		{	"when": { "OR": [
				{"facing": "south", "open": "false", "half": "bottom"}
            ]},
			"apply": { "model": "block/spruce_trapdoor", "y":180}
		},
		{	"when": { "OR": [
				{"facing": "west", "open": "false", "half": "bottom"}
            ]},
			"apply": { "model": "block/spruce_trapdoor", "y":270}
		},
		{	"when": { "OR": [
				{"facing": "north", "open": "false", "half": "top"}
            ]},
			"apply": { "model": "block/spruce_trapdoor_top", "y":180}
		},
		{	"when": { "OR": [
				{"facing": "east", "open": "false", "half": "top"}
            ]},
			"apply": { "model": "block/spruce_trapdoor_top", "y":270}
		},
		{	"when": { "OR": [
				{"facing": "south", "open": "false", "half": "top"}
            ]},
			"apply": { "model": "block/spruce_trapdoor_top"}
		},
		{	"when": { "OR": [
				{"facing": "west", "open": "false", "half": "top"}
            ]},
			"apply": { "model": "block/spruce_trapdoor_top", "y":90}
		}
    ]
}
